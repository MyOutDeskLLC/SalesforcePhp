{
    "name": "myoutdeskllc/salesforce-php",
    "description": "salesforce library for php8+",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "myoutdeskllc\\SalesforcePhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "JL",
            "email": "jay.lindsley@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.4",
        "sammyjo20/saloon": "^0.8.0",
        "league/oauth2-client": "^2.6",
        "league/csv": "^9.8",
        "mihasicehcek/php_salesforce_soql_builder": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "fakerphp/faker": "^1.19"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
